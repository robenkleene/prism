#!/bin/bash

commands=(
    "prism --version"
    "less --version | head -n1"
    "env | grep -iE '(less|pager|bat|prism)'"
    "printf '\e[34m\e[43mtext\e[m\n'"
    "printf '\e[38;5;19m\e[48;5;226mtext\e[m\n'"
    "printf '\e[38;2;0;0;255m\e[48;2;255;255;0mtext\e[m\n'"
    "printf '⋮│─'"
    "prism --no-gitconfig <(echo a) <(echo b) | cat -A"
    "prism  --no-gitconfig <(echo a) <(echo b)"
)

for cmd in "${commands[@]}"; do
    echo "> $cmd"
    eval "$cmd"
    echo
done
